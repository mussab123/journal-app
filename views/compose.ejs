<%-include("partials/header")-%>

<h1 class="compose">Compose</h1>

<form action="/compose" method="post" class="compose">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="value" />
    <label>Post</label>
    <textarea
      class="form-control"
      type="textfield"
      name="textfield"
      rows="10"
      cols="10"
    ></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="submit">Publish</button>
</form>

<%-include("partials/footer")-%>
